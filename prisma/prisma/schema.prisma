generator client {
  provider = "prisma-client-js"
}
datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}
model UserEvent {
  id        String      @id @default(cuid())
  userId    String      @default(cuid())
  createdAt DateTime    @default(now())
  email     String     
  name      String
  isActive  Boolean?
  isAdmin   Boolean?
}
